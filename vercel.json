{
  "version": 2,
  "functions": {
    "build/main.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "templates/(.*)",
      "dest": "templates/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
